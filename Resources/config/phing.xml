<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="rhapsody_phing.phing_home">%kernel.root_dir%/../vendor/phing/phing</parameter>
        <parameter key="rhapsody_phing.phing_classpath">%rhapsody_phing.phing_home%/classes</parameter>
        <parameter key="rhapsody_phing.root_dir">%kernel.root_dir%/..</parameter>
        <parameter key="rhapsody_phing.phing">phing</parameter>
        <parameter key="rhapsody_phing.default_file">build.xml</parameter>
    </parameters>
</container>

{
    "name": "rhapsody-project/phing-bundle",
    "type": "symfony-bundle",
    "description": "Rhapsody Project PhingBundle for Symfony",
    "keywords": ["phing", "rhapsody", "bootstrap"],
    "homepage": "https://github.com/rhapsody-project/phing-bundle",
    "license": "MIT",
    "authors": [
        { "name": "Sean Quinn", "email": "sean.quinn@extesla.com" },
        { "name": "Rhapsody Project", "homepage": "https://github.com/rhapsody-project/phing-bundle/contributors" }
    ],
    "require": {
        "phing/phing": "dev-master",
        "symfony/http-foundation": ">=2.1,<2.3-dev",
        "symfony/form": ">=2.1,<2.3-dev",
        "symfony/validator": ">=2.1,<2.3-dev",
        "symfony/security": ">=2.1,<2.3-dev",
        "symfony/routing": ">=2.1,<2.3-dev",
        "symfony/config": ">=2.1,<2.3-dev",
        "symfony/console": ">=2.1,<2.3-dev"
    },
    "autoload": {
        "psr-0": { "Rhapsody\\PhingBundle": "" }
    },
    "target-dir": "Rhapsody/PhingBundle",
    "minimum-stability": "dev"
}
